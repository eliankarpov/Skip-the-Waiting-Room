<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WorkingHours">

    <TextView
        android:id="@+id/mondayText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Monday:"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/tuesdayText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:text="Tuesday:"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@id/mondayText"
        tools:layout_editor_absoluteX="-4dp" />

    <TextView
        android:id="@+id/wednesdayText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:text="Wednesday:"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@id/tuesdayText"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/thursdayText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="Thursday:"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@id/wednesdayText"
        tools:layout_editor_absoluteX="0dp" />

    <Button
        android:id="@+id/open5"
        android:layout_width="93dp"
        android:layout_height="34dp"
        android:text="Opening"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/close4"
        android:layout_width="93dp"
        android:layout_height="34dp"
        android:text="Closing"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.439" />

    <EditText
        android:id="@+id/editThurs"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:text="Closed"
        app:layout_constraintStart_toStartOf="@id/editMon"
        app:layout_constraintTop_toTopOf="@id/thursdayText" />

    <TextView
        android:id="@+id/fridayText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:text="Friday:"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@id/thursdayText"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/editMon"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:text="Closed"
        app:layout_constraintStart_toStartOf="@id/editTues"
        tools:layout_editor_absoluteY="16dp" />

    <EditText
        android:id="@+id/editTues"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:text="Closed"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tuesdayText"
        app:layout_constraintTop_toTopOf="@id/tuesdayText" />

    <EditText
        android:id="@+id/editWednes"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:text="Closed"
        app:layout_constraintStart_toStartOf="@id/editTues"
        app:layout_constraintTop_toTopOf="@id/wednesdayText" />


    <EditText
        android:id="@+id/editFri"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:text="Closed"
        app:layout_constraintStart_toStartOf="@id/editMon"
        app:layout_constraintTop_toTopOf="@id/fridayText" />

    <Button
        android:id="@+id/open6"
        android:layout_width="93dp"
        android:layout_height="34dp"
        android:text="Opening"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.638" />

    <Button
        android:id="@+id/close5"
        android:layout_width="93dp"
        android:layout_height="34dp"
        android:text="Closing"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.546" />

    <TextView
        android:id="@+id/saturdayText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="Saturday:"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@id/fridayText"
        tools:layout_editor_absoluteX="3dp" />

    <EditText
        android:id="@+id/editSat"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:text="Closed"
        app:layout_constraintStart_toStartOf="@id/editMon"
        app:layout_constraintTop_toTopOf="@id/saturdayText" />

    <TextView
        android:id="@+id/sundayText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="68dp"
        android:text="Sunday:"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@id/saturdayText"
        tools:layout_editor_absoluteX="3dp" />

    <Button
        android:id="@+id/open7"
        android:layout_width="93dp"
        android:layout_height="34dp"
        android:text="Opening"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.77" />

    <EditText
        android:id="@+id/editSun"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:text="Closed"
        app:layout_constraintStart_toStartOf="@id/editMon"
        app:layout_constraintTop_toTopOf="@id/sundayText" />

    <Button
        android:id="@+id/close6"
        android:layout_width="93dp"
        android:layout_height="34dp"
        android:text="Closing"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.687" />

    <Button
        android:id="@+id/updateWorkHrs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="update working hours"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/sundayText"
        app:layout_constraintVertical_bias="0.709" />

    <Button
        android:id="@+id/open1"
        android:layout_width="93dp"
        android:layout_height="34dp"
        android:text="Opening"
        app:layout_constraintBottom_toTopOf="@+id/editTues"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.946"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.242" />

    <Button
        android:id="@+id/close1"
        android:layout_width="93dp"
        android:layout_height="34dp"
        android:text="Closing"
        app:layout_constraintBottom_toTopOf="@+id/editTues"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.943"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.757" />

    <Button
        android:id="@+id/open2"
        android:layout_width="93dp"
        android:layout_height="34dp"
        android:text="Opening"
        app:layout_constraintBottom_toTopOf="@+id/close2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/editTues"
        app:layout_constraintTop_toBottomOf="@+id/close1"
        app:layout_constraintVertical_bias="0.666" />

    <Button
        android:id="@+id/close2"
        android:layout_width="93dp"
        android:layout_height="34dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="20dp"
        android:text="Closing"
        app:layout_constraintBottom_toTopOf="@+id/editWednes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/editTues" />

    <Button
        android:id="@+id/open3"
        android:layout_width="93dp"
        android:layout_height="34dp"
        android:layout_marginEnd="4dp"
        android:text="Opening"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.949"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.263" />

    <Button
        android:id="@+id/open4"
        android:layout_width="93dp"
        android:layout_height="34dp"
        android:text="Opening"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.39" />

    <Button
        android:id="@+id/close7"
        android:layout_width="93dp"
        android:layout_height="34dp"
        android:text="Closing"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.819" />

    <Button
        android:id="@+id/close3"
        android:layout_width="93dp"
        android:layout_height="34dp"
        android:text="Closing"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.312" />
</androidx.constraintlayout.widget.ConstraintLayout>